						<!-- firstHeading -->
			<h1 id="firstHeading" class="firstHeading" lang="nb"><span dir="auto">Svend Foyn</span></h1>
			<!-- /firstHeading -->
			<!-- bodyContent -->
			<div id="bodyContent">
